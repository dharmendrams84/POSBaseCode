<SERIALVALIDATIONSTATUSMAPPER>

    <COMMENT> This xml maps SIM Status to the relevant POS Status</COMMENT>

    <STATUS name="Sellable">IN_STOCK,MISSING,CUSTOMER_FULFILLED,CUSTOMER_RESERVED,IN_RECEIVING,ITEM_NOT_RANGED,INVALID_REQUESTING_LOCATION,INVALID_REQUESTING_STORE,INVALID_ITEM_ID</STATUS>

    <STATUS name="NotSellable">SOLD,SHIPPED_TO_STORE,SHIPPED_TO_WAREHOUSE,SHIPPED_TO_VENDOR,REMOVE_FROM_INVENTORY,UNAVAILABLE,SHIPPED_TO_FINISHER</STATUS>

    <STATUS name="NotAvailable">UIN_DOES_NOT_EXIST</STATUS>

</SERIALVALIDATIONSTATUSMAPPER>